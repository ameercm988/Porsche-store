
<div class="container-fluid pt-4 px-4">
    <div class="row g-4">
        <div class="col-12">
           <h3 class="text mt-3">Add Category</h3>
           <form action="/admin/add-category" method="post" id="form" enctype="multipart/form-data">
            <label for="">Category</label>
            <input type="text" name="category" id="category" class="form-control w-50">
             <label for="">Category Image</label>
            <input type="file" name="categoryimage" id="categoryimage" class="form-control w-50"  style="background-color: aliceblue;" required="">

            <button type="submit" class="btn btn-success mt-3">Submit</button>
           </form>
        </div>
    </div>
</div>


